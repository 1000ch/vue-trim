{
  "name": "vue-trim",
  "version": "0.2.0",
  "description": "Vue.js component for trimming images using Cropper.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "jest",
    "dev": "rollup --watch --config rollup.config.js",
    "build": "rollup --config rollup.config.js"
  },
  "main": "dist/vue-trim.common.js",
  "module": "dist/vue-trim.esm.js",
  "browser": "dist/vue-trim.js",
  "lint-staged": {
    "*.{vue|js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "/test/.+\\.spec\\.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(js)$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1000ch/vue-trim.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "1000ch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/vue-trim/issues"
  },
  "homepage": "https://github.com/1000ch/vue-trim#readme",
  "dependencies": {
    "cropperjs": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.10",
    "vue-jest": "github:vuejs/vue-jest",
    "vue-template-compiler": "^2.6.0"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
